{
  "name": "minimal-example",
  "version": "1.0.0",
  "type": "module",
  "description": "Minimal SSOT Codegen example - simplest possible setup",
  "private": true,
  "scripts": {
    "setup": "pnpm install && pnpm generate && pnpm db:push",
    "generate": "node scripts/generate.js",
    "db:push": "prisma db push",
    "db:seed": "tsx scripts/seed.ts",
    "test": "tsx src/test-consumer.ts",
    "clean": "rimraf gen/ dist/ node_modules/"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "@ssot-codegen/gen": "workspace:*",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.9.0"
  }
}
