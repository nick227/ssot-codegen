{
  "name": "ssot-image-optimizer-example",
  "version": "1.0.0",
  "description": "Image conversion and optimization API example using SSOT-Codegen",
  "private": true,
  "scripts": {
    "dev": "node generated/server.js",
    "start": "NODE_ENV=production node generated/server.js",
    "generate": "pnpm ssot-gen --schema schema.prisma --config ssot.config.js",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "test": "node test-api.js"
  },
  "keywords": [
    "image-conversion",
    "image-optimization",
    "ffmpeg",
    "png-to-jpg",
    "webp",
    "avif",
    "ssot-codegen"
  ],
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "express": "^4.18.0",
    "fluent-ffmpeg": "^2.1.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "prisma": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

