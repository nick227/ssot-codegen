{
  "name": "enterprise-api-example",
  "version": "1.0.0",
  "description": "Enterprise API demonstrating all 5 advanced features",
  "type": "module",
  "scripts": {
    "generate": "USE_REGISTRY=true node ../../packages/gen/dist/cli.js --schema schema.prisma --output src",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
