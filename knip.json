{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "packages/gen/src/index-new.ts",
    "packages/cli/src/index.ts"
  ],
  "project": [
    "packages/gen/src/**/*.ts",
    "packages/cli/src/**/*.ts"
  ],
  "ignore": [
    "**/__tests__/**",
    "**/*.test.ts",
    "**/dist/**",
    "generated/**",
    "examples/**",
    "scripts/**",
    "**/node_modules/**"
  ],
  "ignoreDependencies": [
    "@ssot-codegen/sdk-runtime",
    "@ssot-codegen/templates-default",
    "@ssot-codegen/core"
  ],
  "ignoreExportsUsedInFile": true
}
